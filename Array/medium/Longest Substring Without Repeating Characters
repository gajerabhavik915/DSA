class Solution:
    def lengthOfLongestSubstring(self, s: str) -> int:
        set1 = set()
        start = 0
        res = 0
        for end in range(len(s)):
            while s[end] in set1:
                set1.remove(s[start])
                start = start + 1
            set1.add(s[end])
            res = max(res,(end - start + 1))

        return (res)
